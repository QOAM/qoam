@model QOAM.Website.ViewModels.Import.UpdatedViewModel

@using QOAM.Website.Resources

@{
    ViewBag.Title = ViewStrings.Admin_Updated_Title;
}

<div class="container main">

    <p>
        @ViewStrings.Admin_Updated_Success:
    </p>
    
    <ul>
        @foreach(var foundISSN in Model.FoundISSNs)
        {
            <li>@foundISSN</li>
        }
    </ul>

  <p>
        @ViewStrings.Admin_Updated_Failure:
    </p>
    
    <ul>
        @foreach(var notFoundISSN in Model.NotFoundISSNs)
        {
            <li>@notFoundISSN</li>
        }
    </ul>
</div>
