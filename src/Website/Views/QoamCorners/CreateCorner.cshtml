@model QOAM.Website.ViewModels.QoamCorners.CornerImportViewModel

@{
    ViewBag.Title = "Create new QOAMcorners";
}

<div class="container main">

    <h2>@ViewBag.Title</h2>

    <p>
        Select an Excel file containing one or more QOAMcorner definitions (one per column).
    </p>
    <br />
    @using (Html.BeginForm("CreateCorner", "QoamCorners", FormMethod.Post, new { role = "form", enctype = "multipart/form-data" }))
    {
        @Html.AntiForgeryToken()

        <fieldset>
            <legend>@ViewBag.Title</legend>

            @Html.Partial("_FileUpload")

            <div class="row bottommargin">
                <div class="col-xs-1 vcenter">
                </div>
                <div class="col-xs-11">
                    <input type="submit" value="Import" class="btn btn-primary" />
                </div>
            </div>
        </fieldset>
    }
</div>

@section scripts
{
    @Html.Partial("_FormScripts")
}