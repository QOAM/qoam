@model QOAM.Website.ViewModels.Import.ProcessedISSNsViewModel

@{
    ViewBag.Title = string.Format("{0} label process result", Model.LabelType);
}

<div class="container main">
    
    @if (Model.FoundISSNs.Any()) {
        <p>
            The @Model.LabelType label has been @Model.CustomMessage the journals with the following ISSNs:
        </p>

        <ul>
            @foreach (var foundISSN in Model.FoundISSNs) {
                <li>@foundISSN</li>
            }
        </ul>
    }
    
    @if (Model.NotFoundISSNs.Any()) {
        <p>
            The journals with the following ISSNs were not processed as they could not be found:
        </p>

        <ul>
            @foreach (var notFoundISSN in Model.NotFoundISSNs) {
                <li>@notFoundISSN</li>
            }
        </ul>
    }
</div>