@model QOAM.Website.ViewModels.Import.UpdatedViewModel

@{
    ViewBag.Title = "Journals update result";
}

<div class="container main">

    <p>
        The journals with the following ISSNs were successfully updated:
    </p>
    
    <ul>
        @foreach (var foundISSN in Model.FoundISSNs)
        {
            <li>@foundISSN</li>
        }
    </ul>

    <p>
        The journals with the following ISSNs were not updated as they could not be found:
    </p>
    
    <ul>
        @foreach (var notFoundISSN in Model.NotFoundISSNs)
        {
            <li>@notFoundISSN</li>
        }
    </ul>
</div>