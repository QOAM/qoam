@model QOAM.Website.ViewModels.Import.ImportedViewModel

@{
    ViewBag.Title = "Journals import result";
}

<div class="container main">

    <p>
    The journal with the following ISSNs were successfully imported:
    </p>

    <ul>
        @foreach (var foundISSN in Model.FoundISSNs)
        {
            <li>@foundISSN</li>
        }
    </ul>

    <p>
    The journal with the following ISSNs were not imported as they could not be found:
    </p>

    <ul>
        @foreach (var notFoundISSN in Model.NotFoundISSNs)
        {
            <li>@notFoundISSN</li>
        }
    </ul>
</div>